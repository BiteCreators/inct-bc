{
  "name": "inctagram",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NEXT_PRIVATE_LOCAL_WEBPACK=true next dev -p 3000",
    "build": "NEXT_PRIVATE_LOCAL_WEBPACK=true next build",
    "start": "next start",
    "format": "prettier --write src",
    "lint": "eslint --fix src/**/*.{tsx,ts,jsx,js} --no-error-on-unmatched-pattern && stylelint --fix src/{,*/}*.{scss,css} --allow-empty-input",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "rename-icons": "node scripts/rename-icons.cjs",
    "replace-colors": "node scripts/replace-colors.cjs",
    "generate-icons": "pnpm run rename-icons && npx @svgr/cli src/common/assets/icons/svg --out-dir src/common/assets/icons/components --jsx-runtime automatic --typescript --ref --memo && pnpm run replace-colors && pnpm run format && pnpm run lint",
    "asd": "tsc --noEmit",
    "test": "jest"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@module-federation/nextjs-mf": "^8.7.8",
    "@packages/shared": "^1.0.0",
    "@reduxjs/toolkit": "^2.2.7",
    "@types/dom-to-image": "^2.6.7",
    "@types/react-google-recaptcha": "^2.1.9",
    "@types/react-select": "^5.0.1",
    "async-mutex": "^0.5.0",
    "date-fns": "^4.1.0",
    "dom-to-image": "^2.6.0",
    "jose": "^5.9.4",
    "next": "14.2.7",
    "next-redux-wrapper": "^8.1.0",
    "react-cookie": "^7.2.0",
    "react-day-picker": "^9.1.2",
    "react-easy-crop": "^5.1.0",
    "react-google-recaptcha": "^3.1.0",
    "react-google-recaptcha-v3": "^1.10.1",
    "react-image-crop": "^11.0.7",
    "react-redux": "^9.1.2",
    "react-select": "^5.8.1",
    "react-swipeable": "^7.0.1",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^2.5.2",
    "webpack": "^5.96.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "1.8.0",
    "@types/nprogress": "^0.2.3",
    "msw": "^2.6.4",
    "postcss": "^8",
    "tailwindcss": "^3.4.1"
  },
  "workspaces": [
    "apps/*",
    "shared/*"
  ]
}
